#version 100

uniform mat4 WVP;
uniform float texUnpack;

attribute vec4 pos;
attribute vec3 ipos;
varying vec2 texCoord;
attribute vec2 tex;

void main()
{
    vec4 spos = vec4(pos.xyz, 1.0);
    vec3 _25 = spos.xyz + ipos;
    spos = vec4(_25.x, _25.y, _25.z, spos.w);
    gl_Position = WVP * spos;
    texCoord = tex * texUnpack;
}

