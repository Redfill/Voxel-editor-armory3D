#version 300 es
precision mediump float;
precision highp int;

uniform highp vec3 DiffuseBSDFColor;

out highp vec4 fragColor;

void main()
{
    highp vec3 basecol = DiffuseBSDFColor;
    highp float roughness = 0.0;
    highp float metallic = 0.0;
    highp float occlusion = 1.0;
    highp float specular = 0.0;
    fragColor = vec4(basecol, 1.0);
    highp vec3 _32 = pow(fragColor.xyz, vec3(0.4545454680919647216796875));
    fragColor = vec4(_32.x, _32.y, _32.z, fragColor.w);
}

